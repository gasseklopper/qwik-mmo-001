{
	"name": "my-qwik-docs-starter",
	"description": "Documentation site starter",
	"engines": {
		"node": "^18.17.0 || ^20.3.0 || >=21.0.0"
	},
	"engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
	"trustedDependencies": [
		"sharp"
	],
	"private": true,
	"trustedDependencies-annotation": "Needed for bun to allow running install scripts",
	"type": "module",
	"scripts": {
		"build": "qwik build",
		"build-storybook": "storybook build -- -o public/storybook",
		"build.client": "vite build",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.server": "vite build -c adapters/netlify-edge/vite.config.ts",
		"build.types": "tsc --incremental --noEmit",
		"coverage": "vitest run --coverage",
		"deploy": "netlify deploy --build",
		"dev": "vite --mode ssr",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"lint": "eslint \"src/**/*.ts*\"",
		"preview": "qwik build preview && vite preview --open",
		"serve": "netlify dev",
		"start": "vite --open --mode ssr",
		"storybook": "storybook dev -p 6006",
		"test": "vitest --run",
		"test.unit": "vitest components",
		"test.unit.ui": "vitest --ui components",
		"test.watch": "vitest",
		"qwik": "qwik"
	},
	"devDependencies": {
		"@builder.io/partytown": "^0.10.3",
		"@builder.io/qwik": "^1.13.0",
		"@builder.io/qwik-city": "^1.13.0",
		"@netlify/edge-functions": "^2.0.0",
		"@netlify/plugin-lighthouse": "^6.0.1",
		"@netlify/vite-plugin-netlify-edge": "1.1.3",
		"@qwik-ui/headless": "^0.6.6",
		"@qwikest/icons": "^0.0.13",
		"@supabase/supabase-js": "^2.49.4",
		"@types/eslint": "9.6.1",
		"@types/gsap": "^3.0.0",
		"@types/locomotive-scroll": "^4.1.4",
		"@types/node": "20.14.11",
		"@typescript-eslint/eslint-plugin": "7.16.1",
		"@typescript-eslint/parser": "7.16.1",
		"@vitest/ui": "^0.34.6",
		"eslint": "8.57.0",
		"eslint-plugin-qwik": "^1.13.0",
		"gsap": "^3.12.7",
		"locomotive-scroll": "^4.1.4",
		"netlify-cli": "^15.0.0",
		"prettier": "3.3.3",
		"sass": "^1.86.3",
		"typescript": "5.4.5",
		"undici": "*",
		"vite": "6.3.2",
		"vite-tsconfig-paths": "^4.2.1",
		"vitest": "^0.34.6",
		"storybook-framework-qwik": "^0.4.2"
	}
}